<div class="ff-breadcrumb-trail">
    <span class="description">{l s='Your search for:' mod='factfinder'}</span>

    <ff-breadcrumb-trail unresolved>
        <ff-breadcrumb-trail-item type="all">
            <span>{literal}{{text}}{/literal}</span>
        </ff-breadcrumb-trail-item>
    </ff-breadcrumb-trail>

    <ff-template scope="result" unresolved>
        <span>{l s='produced {{resultCount}} results' mod='factfinder'}</span>
    </ff-template>
</div>
